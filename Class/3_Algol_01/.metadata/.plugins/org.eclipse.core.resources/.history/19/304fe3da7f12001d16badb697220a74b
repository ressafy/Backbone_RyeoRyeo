package 안려환;

import java.util.Scanner;

public class P007_SWEA1954_달팽이숫자 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		
		for(int test = 0; test<N; test++) {
			int cnt = 1, dir = 0;
			int T = sc.nextInt();
			int [][] arr = new int [T][T];
			
			for(int i = 0; i< T; i++) {
				for(int j = 0; j<T; j++) {
					if(cnt <= T) {
						arr[i][j] = cnt;
						cnt++;
					}else {
						
					}
				}
			}
		}

	}

}
